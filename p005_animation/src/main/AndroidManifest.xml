<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.studyandroid.animation">

    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.TagCloudEasyhomeActivity" />
        <activity android:name=".activities.TagCloudDeviceBxActivity">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />

                <action android:name="hs.act.github.phone.TagCloudDeviceBxActivity" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.TagCloudDeviceRqesqActivity">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />

                <action android:name="hs.act.github.phone.TagCloudDeviceRqesqActivity" />
            </intent-filter>
        </activity>
    </application>

</manifest>